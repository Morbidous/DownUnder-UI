#---------------------------------- Content ---------------------------------#

#begin DownUnder Native Content/Effects/Blur.fx
/importer:EffectImporter
/processor:EffectProcessor
/processorParam:DebugMode=Auto
/build:DownUnder Native Content/Effects/Blur.fx

#begin DownUnder Native Content/Effects/Gradient.fx
/importer:EffectImporter
/processor:EffectProcessor
/processorParam:DebugMode=Auto
/build:DownUnder Native Content/Effects/Gradient.fx

#begin DownUnder Native Content/Effects/Lens.fx
/importer:EffectImporter
/processor:EffectProcessor
/processorParam:DebugMode=Auto
/build:DownUnder Native Content/Effects/Lens.fx

#begin DownUnder Native Content/Images/Blurred Circle 512.png
/importer:TextureImporter
/processor:TextureProcessor
/processorParam:ColorKeyColor=255,0,255,255
/processorParam:ColorKeyEnabled=True
/processorParam:GenerateMipmaps=False
/processorParam:PremultiplyAlpha=True
/processorParam:ResizeToPowerOfTwo=False
/processorParam:MakeSquare=False
/processorParam:TextureFormat=Color
/build:DownUnder Native Content/Images/Blurred Circle 512.png

#begin DownUnder Native Content/SpriteFonts/Default Font.spritefont
/importer:FontDescriptionImporter
/processor:FontDescriptionProcessor
/processorParam:PremultiplyAlpha=True
/processorParam:TextureFormat=Compressed
/build:DownUnder Native Content/SpriteFonts/Default Font.spritefont

